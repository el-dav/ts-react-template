{
  "name": "ts-react-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.10",
    "material-ui": "^1.0.0-beta.44",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-ink": "^6.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-redux": "^4.0.8",
    "react-scripts-ts": "2.15.1",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.6.0",
    "redux-observable": "^0.17.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.8",
    "styled-components": "^3.2.6"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css":
      "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "prettify": "prettier --write \"./**/*.tsx\"",
    "react-devtools": "react-devtools",
    "dev": "concurrently \"npm run react-devtools \" \"npm start\""
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.0.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "@types/react-router": "^3.0.15",
    "@types/react-router-redux": "^4.0.8",
    "@types/react-tap-event-plugin": "^0.0.30",
    "@types/react-transition-group": "^2.0.9",
    "@types/webpack": "^4.1.4",
    "@types/webpack-env": "^1.13.6",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "node-sass-chokidar": "^1.2.2",
    "prettier": "^1.12.1",
    "react-devtools": "^3.2.1",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-slider-monitor": "^2.0.0-1",
    "rxjs-tslint-rules": "^4.1.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  }
}
